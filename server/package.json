{
  "name": "vision-for-everyone",
  "version": "1.0.0",
  "description": "Kiwe Chatbot",
  "main": "server.js",
  "engine": {
    "node": "13.13.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "start": "node server.js",
    "backend": "nodemon server.js",
    "frontend": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "style-check": "npm run linter && npm run formatter",
    "linter": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "formatter": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Sungpyo Cho",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/dialogflow": "^3.3.0",
    "actions-on-google": "^2.12.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "mongodb-memory-server": "^7.4.0",
    "mongoose": "^5.9.28",
    "nodemailer": "^6.4.16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6"
  }
}
